{
    "name": "gd_peter/tp6-addons",
    "type": "library",
    "require": {
        "php": ">= 7.1.0",
        "topthink/framework": "^6.0.0",
        "myttyy/directory-file": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "GdPeter\\Tp6Addons\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "app\\": "app"
        }
    },
    "authors": [
        {
            "name": "peter",
            "email": "youngpeter@foxmain.com"
        }
    ],
    "extra": {
        "think":{
            "services":[
                "GdPeter\\Tp6Addons\\Service"
            ],
            "config":{
                "package": "src/config.php"
            }
        }
    },
    "scripts" : {
        "post-install-cmd": [
            "php think package:config"
        ]
    },
    "license": "MIT"
}
